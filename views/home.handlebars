<div class="container">

  {{#each hotels}}
    <div class="card">
      <div class="card-header">
        Featured
      </div>
      <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <p class="card-text">{{this.price}}</p>
        <a href="#" class="btn btn-primary">{{this.city}}</a>
      </div>
    </div>
  {{/each}}
</div>